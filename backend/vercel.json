{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/signup", "dest": "/public/signup.html" },
    { "src": "/download", "dest": "/public/download.html" },
    { "src": "/success", "dest": "/public/success.html" },
    { "src": "/cancel", "dest": "/public/cancel.html" },
    { "src": "/privacy", "dest": "/public/privacy.html" },
    { "src": "/terms", "dest": "/public/terms.html" },
    { "src": "/css/(.*)", "dest": "/public/css/$1" },
    { "src": "/js/(.*)", "dest": "/public/js/$1" },
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "^/$", "dest": "/public/index.html" }
  ],
  "headers": []
}
